              categories : null,\n            }\n        },\n        methods: {\n            async getCategories() {\n                await axios.get(this.baseURL + \"category/\")\n                    .then(res => this.categories = res.data)\n                    .catch(err => console.log(err))\n            }\n        },\n        mounted(){\n            this.getCategories();\n        }\n    }\n</script>\n\n<style scoped>\n    h4 {\n        font-family: 'Roboto', sans-serif;\n        color: #484848;\n        font-weight: 700;\n    }\n\n    #add-category {\n        float: right;\n        font-weight: 500;\n    }\n</style>"],"mappings":"AAyBI,IAAIA,KAAI,GAAKC,OAAO,CAAC,OAAO,CAAC;AAC7B,eAAe;EACXC,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,OAAM,EAAK,wBAAwB;MACnCC,UAAS,EAAI;IACjB;EACJ,CAAC;EACDC,OAAO,EAAE;IACL,MAAMC,aAAaA,CAAA,EAAG;MAClB,MAAMP,KAAK,CAACQ,GAAG,CAAC,IAAI,CAACJ,OAAM,GAAI,WAAW,EACrCK,IAAI,CAACC,GAAE,IAAK,IAAI,CAACL,UAAS,GAAIK,GAAG,CAACP,IAAI,EACtCQ,KAAK,CAACC,GAAE,IAAKC,OAAO,CAACC,GAAG,CAACF,GAAG,CAAC;IACtC;EACJ,CAAC;EACDG,OAAOA,CAAA,EAAE;IACL,IAAI,CAACR,aAAa,CAAC,CAAC;EACxB;AACJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}