{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.json.stringify.js\";\nvar axios = require('axios');\nexport default {\n  data() {\n    return {\n      id: null,\n      categoryId: 0,\n      name: null,\n      description: null,\n      imageURL: null,\n      price: 0,\n      productIndex: null\n    };\n  },\n  props: [\"baseURL\", \"products\", \"categories\"],\n  methods: {\n    async editProduct() {\n      const updatedProduct = {\n        id: this.id,\n        categoryId: this.categoryId,\n        name: this.name,\n        description: this.description,\n        imageURL: this.imageURL,\n        price: this.price\n      };\n      await axios({\n        method: 'post',\n        url: this.baseURL + \"product/\" + this.id,\n        data: JSON.stringify(updatedProduct),\n        headers: {\n          'Content-Type': 'application/json'\n        }\n      }).then(() => {\n        this.$emit(\"fetchData\");\n        this.$router.push({\n          name: 'ListProduct'\n        });\n        console.log(\"Product Updated Successfully!\");\n      }).catch(err => console.log(\"Hello\", err));\n    }\n  },\n  mounted() {\n    this.id = this.$route.params.id;\n    this.productIndex = this.products.findIndex(product => product.id == this.id);\n    this.categoryId = this.products[this.productIndex].categoryId;\n    this.name = this.products[this.productIndex].name;\n    this.description = this.products[this.productIndex].description;\n    this.imageURL = this.products[this.productIndex].imageURL;\n    this.price = this.products[this.productIndex].price;\n  }\n};","map":{"version":3,"names":["axios","require","data","id","categoryId","name","description","imageURL","price","productIndex","props","methods","editProduct","updatedProduct","method","url","baseURL","JSON","stringify","headers","then","$emit","$router","push","console","log","catch","err","mounted","$route","params","products","findIndex","product"],"sources":["/Users/maheshbiradar/Documents/EKS-installation/java-fullstack-project/frontend/fe-ecommerce/src/views/Product/EditProduct.vue"],"sourcesContent":["<template>\n    <div class=\"container\">\n      <div class=\"row\">\n        <div class=\"col-12 text-center\">\n          <h4 class=\"pt-3\">Edit Product</h4>\n        </div>\n      </div>\n  \n      <div class=\"row\">\n        <div class=\"col-3\"></div>\n        <div class=\"col-md-6 px-5 px-md-0\">\n          <form>\n            <div class=\"form-group\">\n              <label>Category</label>\n              <select class=\"form-control\" v-model=\"categoryId\" required>\n                <option v-for=\"category of categories\" :key=\"category.id\" :value=\"category.id\">{{category.categoryName}}</option>\n              </select>\n            </div>\n            <div class=\"form-group\">\n              <label>Name</label>\n              <input type=\"text\" class=\"form-control\" v-model=\"name\" required>\n            </div>\n            <div class=\"form-group\">\n              <label>Description</label>\n              <input type=\"text\" class=\"form-control\" v-model=\"description\" required>\n            </div>\n            <div class=\"form-group\">\n              <label>ImageURL</label>\n              <input type=\"url\" class=\"form-control\" v-model=\"imageURL\" required>\n            </div>\n            <div class=\"form-group\">\n              <label>Price</label>\n              <input type=\"number\" class=\"form-control\" v-model=\"price\" required>\n            </div>\n            <button type=\"button\" class=\"btn btn-primary\" @click=\"editProduct\">Submit</button>\n          </form>\n        </div>\n        <div class=\"col-3\"></div>\n      </div>\n    </div>\n  </template>\n  \n  <script>\n  var axios =  require('axios');\n  export default {\n    data(){\n      return {\n        id : null,\n        categoryId : 0,\n        name : null,\n        description : null,\n        imageURL : null,\n        price : 0,\n        productIndex : null\n      }\n    },\n    props : [\"baseURL\", \"products\", \"categories\"],\n    methods : {\n      async editProduct() {\n        const updatedProduct = {\n          id : this.id,\n          categoryId : this.categoryId,\n          name : this.name,\n          description : this.description,\n          imageURL : this.imageURL,\n          price : this.price\n        }\n  \n        await axios({\n          method: 'post',\n          url: this.baseURL+\"product/\"+this.id,\n          data : JSON.stringify(updatedProduct),\n          headers: {\n            'Content-Type': 'application/json'\n          }\n        })\n        .then(() => {\n          this.$emit(\"fetchData\");\n          this.$router.push({name : 'ListProduct'});\n          console.log(\"Product Updated Successfully!\")\n        })\n        .catch(err => console.log(\"Hello\", err));\n      }\n    },\n    mounted() {\n      this.id = this.$route.params.id;\n      this.productIndex = this.products.findIndex(product => product.id == this.id);\n      this.categoryId = this.products[this.productIndex].categoryId;\n      this.name = this.products[this.productIndex].name;\n      this.description = this.products[this.productIndex].description;\n      this.imageURL = this.products[this.productIndex].imageURL;\n      this.price = this.products[this.productIndex].price;\n    }\n  }\n  </script>\n  \n  <style scoped>\n  h4 {\n    font-family: 'Roboto', sans-serif;\n    color: #484848;\n    font-weight: 700;\n  }\n  \n  </style>\n  "],"mappings":";;AA2CE,IAAIA,KAAI,GAAKC,OAAO,CAAC,OAAO,CAAC;AAC7B,eAAe;EACbC,IAAIA,CAAA,EAAE;IACJ,OAAO;MACLC,EAAC,EAAI,IAAI;MACTC,UAAS,EAAI,CAAC;MACdC,IAAG,EAAI,IAAI;MACXC,WAAU,EAAI,IAAI;MAClBC,QAAO,EAAI,IAAI;MACfC,KAAI,EAAI,CAAC;MACTC,YAAW,EAAI;IACjB;EACF,CAAC;EACDC,KAAI,EAAI,CAAC,SAAS,EAAE,UAAU,EAAE,YAAY,CAAC;EAC7CC,OAAM,EAAI;IACR,MAAMC,WAAWA,CAAA,EAAG;MAClB,MAAMC,cAAa,GAAI;QACrBV,EAAC,EAAI,IAAI,CAACA,EAAE;QACZC,UAAS,EAAI,IAAI,CAACA,UAAU;QAC5BC,IAAG,EAAI,